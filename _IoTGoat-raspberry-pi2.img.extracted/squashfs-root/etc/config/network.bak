config interface 'loopback'
    option ifname 'lo'
    option proto 'static'
    option ipaddr '127.0.0.1'
    option netmask '255.0.0.0'

config interface 'lan'
    option ifname 'eth0'
    option type 'bridge'
    option proto 'static'
    option ipaddr '192.168.99.1'
    option netmask '255.255.255.0'
    option dns '8.8.8.8'
    option ip6addr 'fdca:1:2:3:4::1234/64'
    option ip6gw 'fdca:1:2:3:4::1'
    option ip6assign '60'

config interface 'wan'
    option proto 'dhcp'
    option force_link '1'
    option ifname 'eth1'
    option hostname 'IoTGoat'
    option dns '8.8.8.8'
