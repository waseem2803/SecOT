config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
        option ifname 'eth0'
        option type 'bridge'
        option proto 'dhcp'

config interface 'wan'
	option proto 'dhcp'
	option force_link '1'
	option ifname 'eth1'
	option hostname 'IoTGoat'
	option dns '8.8.8.8'
